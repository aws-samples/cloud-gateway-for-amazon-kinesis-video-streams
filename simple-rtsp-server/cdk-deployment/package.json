{
  "name": "simple-rtsp-server-cdk",
  "version": "0.1.0",
  "description": "Serverless RTSP server deployment using AWS CDK",
  "bin": {
    "simple-rtsp-server-cdk": "bin/simple-rtsp-server.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy": "npm run build && cdk deploy --profile malone-aws --require-approval never",
    "destroy": "cdk destroy --profile malone-aws --force",
    "bootstrap": "cdk bootstrap --profile malone-aws"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "18.14.6",
    "jest": "^29.5.0",
    "ts-jest": "^29.0.5",
    "aws-cdk": "2.87.0",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.5"
  },
  "dependencies": {
    "aws-cdk-lib": "2.87.0",
    "constructs": "^10.0.0"
  }
}
